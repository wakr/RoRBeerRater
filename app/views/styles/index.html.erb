<p id="notice"><%= notice %></p>

<h1>Listing Styles</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="6"></th>
    </tr>
  </thead>

  <tbody>
    <% @styles.each do |style| %>
      <tr>
        <td><%= link_to style.name, style %></td>
        <td></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>


